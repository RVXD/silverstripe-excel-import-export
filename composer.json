{
    "name": "lekoala/silverstripe-excel-import-export",
    "description": "Replace CSV with Excel in SilverStripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "excel",
        "import",
        "export",
        "module",
        "cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "LeKoala",
            "email": "thomas@lekoala.be"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "silverstripe/recipe-plugin": "^1 || ^2",
        "silverstripe/vendor-plugin": "^1 || ^2",
        "silverstripe/framework": "^4.6 || ^5",
        "phpoffice/phpspreadsheet": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "silverstripe/siteconfig": "^4.6|^5",
        "silverstripe/admin": "^1.6|^2"
    },
    "extra": {
        "installer-name": "excel-import-export"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "support": {
        "issues": "https://github.com/lekoala/silverstripe-excel-import-export/issues"
    },
    "autoload": {
        "psr-4": {
            "LeKoala\\ExcelImportExport\\": "src/",
            "LeKoala\\ExcelImportExport\\Test\\": "tests/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/",
        "lint-clean": "phpcbf src/ tests/",
        "test": "phpunit -v"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
